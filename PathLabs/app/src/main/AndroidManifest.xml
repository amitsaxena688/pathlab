<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pathlabs" >
    <application
        android:name="base.PathLabsApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/myTheme">

        <activity
            android:name="pathlabs.com.pathlabs.UserAuthentication.Controler.Ui.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="pathlabs.com.pathlabs.Main.MainActivity"
            android:label="@string/dr_labs"></activity>
    </application>

</manifest>
